{
  "name": "navigator-admin-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "tsc && vite build",
    "check-formatting": "prettier --check . ",
    "format": "prettier --write . ",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "markdownlint": "markdownlint --ignore node_modules .",
    "precommit": "yarn format && yarn markdownlint && yarn lint",
    "preview": "vite preview",
    "prod": "tsc && vite build && vite preview --port 3000 --host",
    "prepare": "husky install",
    "test": "jest",
    "find-prettier": "prettier --find-config-path",
    "validate-formatting": "prettier . --check --config ./.prettierrc --max-warnings=0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.1",
    "axios": "^1.4.0",
    "chakra-react-select": "^4.7.3",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "framer-motion": "^10.12.17",
    "html-to-draftjs": "^1.5.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^7.46.1",
    "react-router-dom": "^6.14.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/draft-js": "^0.11.14",
    "@types/draftjs-to-html": "^0.8.3",
    "@types/html-to-draftjs": "^1.4.2",
    "@types/jest": "^29.5.11",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-draft-wysiwyg": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli": "^0.37.0",
    "prettier": "^3.1.0",
    "react-icons": "^4.10.1",
    "ts-jest": "^29.1.2",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.4.0"
  }
}
